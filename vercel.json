{
  "version": 2,
  "builds": [
    { "src": "api/*.js", "use": "@vercel/node" },
    { "src": "public/**/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1.js" },
    { "src": "/reset", "dest": "/public/reset.html" },
    { "src": "/sukses", "dest": "/public/sukses.html" },
    { "src": "/", "dest": "/public/index.html" }
  ]
}
