{
  "version": 2,
  "builds": [
    { "src": "pages/api/*.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/pages/api/$1.js" },
    { "src": "/reset", "dest": "/reset.html" },
    { "src": "/sukses", "dest": "/sukses.html" },
    { "src": "/", "dest": "/index.html" }
  ]
}
